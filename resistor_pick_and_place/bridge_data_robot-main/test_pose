import pyrobot

# Initialize the robot (LoCoBot in this example)
robot = pyrobot.LoCoBot()

# Define the target pose
# This example uses position (x, y, z) and quaternion orientation (x, y, z, w)
target_position = [0.4, 0.1, 0.2]  # Example position in meters
target_orientation_quat = [0.0, 0.0, 0.0, 0.0] # Example orientation (identity quaternion, no rotation)

# Move the end-effector to the specified pose
# 'plan=True' uses MoveIt for collision avoidance
robot.arm.set_ee_pose(target_position, target_orientation_quat, plan=True)
